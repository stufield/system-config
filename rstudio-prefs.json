{
    "panes": {
        "quadrants": [
            "Source",
            "TabSet1",
            "Console",
            "TabSet2"
        ],
        "tabSet1": [
            "Plots",
            "Presentation",
            "Tutorial"
        ],
        "tabSet2": [
            "Environment",
            "History",
            "Files",
            "Connections",
            "Packages",
            "Help",
            "Build",
            "VCS",
            "Viewer"
        ],
        "console_left_on_top": false,
        "console_right_on_top": true
    },
    "show_indent_guides": true,
    "continue_comments_on_newline": true,
    "editor_keybindings": "vim",
    "insert_matching": false,
    "check_arguments_to_r_function_calls": true,
    "check_unexpected_assignment_in_function_call": true,
    "warn_if_no_such_variable_in_scope": true,
    "auto_discover_package_dependencies": false,
    "auto_append_newline": true,
    "strip_trailing_whitespace": true,
    "soft_wrap_r_files": true,
    "syntax_color_console": true,
    "scroll_past_end_of_document": true,
    "highlight_r_function_calls": true,
    "save_files_before_build": true,
    "font_size_points": 11,
    "editor_theme": "Solarized Light",
    "default_project_location": "~/github",
    "pdf_previewer": "rstudio",
    "show_hidden_files": true,
    "wrap_tab_navigation": false,
    "git_diff_ignore_whitespace": true,
    "save_workspace": "never",
    "always_save_history": false,
    "remove_history_duplicates": true,
    "reuse_sessions_for_project_links": true,
    "use_secure_download": false,
    "cran_mirror": {
        "name": "Global (CDN)",
        "host": "RStudio",
        "url": "http://cran.rstudio.com/",
        "secondary": "",
        "country": "us"
    },
    "posix_terminal_shell": "bash"
}